{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["esnext"],
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["google-apps-script"]
  },
  "include": [
    "src/**/*.ts"
  ]
}
